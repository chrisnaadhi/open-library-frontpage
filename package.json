{
  "name": "oplib",
  "version": "1.0.0",
  "description": "Open Library FrontPage ReDesign Project",
  "main": "index.js",
  "scripts": {
    "build-watch": "sass --watch --no-source-map src/sass/styles.scss:src/styles.css",
    "build": "sass --no-source-map src/sass/styles.scss:src/styles.css",
    "css-build": "node-sass --omit-source-map-url src/sass/styles.scss src/styles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisnaadhi/open-library-frontpage.git"
  },
  "author": "Chrisna Adhi Pranoto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisnaadhi/open-library-frontpage/issues"
  },
  "homepage": "https://github.com/chrisnaadhi/open-library-frontpage#readme",
  "devDependencies": {
    "bulma": "^0.9.3",
    "node-sass": "^6.0.1"
  }
}
